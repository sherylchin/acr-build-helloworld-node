{
  "name": "helloworld",
  "version": "1.0.0",
  "description": "Sample app for ACR Build",
  "license": "MIT",
  "main": "server.js",
  "dependencies": {
      "esbuild": "^0.23.0",
      "express": "^4.18.2"
  },
  "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/node": "^20.14.12"
  },
  "scripts": {
    "build": "esbuild server.js --bundle --platform=node --outfile=server-out.js",
    "generate-blob": "node --experimental-sea-config sea-config.json",
    "copy-node": "mkdir release && cp $(command -v node) release/tefi-proxy",
    "inject-blob": "npx postject release/tefi-proxy NODE_SEA_BLOB sea-prep.blob --sentinel-fuse NODE_SEA_FUSE_fce680ab2cc467b6e072b8b5df1996b2",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js"
  },
  "engines": {
      "node": ">=20.0.0 < 21.0.0"
  }
}
